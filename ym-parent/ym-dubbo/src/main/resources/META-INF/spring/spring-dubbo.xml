<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo
		http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="ymDubbo-service"/>

    
    <dubbo:registry address="192.168.238.129:2181,192.168.238.131:2181,192.168.238.132:2181" protocol="zookeeper"/>
    
    <dubbo:protocol name="dubbo" port="28888"/>

    <dubbo:service interface="com.ym.dubbo.service.MenuDubboService" ref="menuDubboServiceImpl"/>
    <dubbo:service interface="com.ym.dubbo.service.UserDubboService" ref="userDubboServiceImpl"/>

    <bean id="menuDubboServiceImpl" class="com.ym.dubbo.service.impl.MenuDubboServiceImpl"/>
    <bean id="userDubboServiceImpl" class="com.ym.dubbo.service.impl.UserDubboServiceImpl"/>


    <import resource="../../spring-core.xml"/>

</beans>